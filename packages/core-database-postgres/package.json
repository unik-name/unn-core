{
    "name": "@arkecosystem/core-database-postgres",
    "version": "5.3.0-dev.0",
    "description": "PostgreSQL integration for ARK Core",
    "license": "MIT",
    "contributors": [
        "Brian Faust <brian@ark.io>"
    ],
    "files": [
        "dist"
    ],
    "main": "dist/index",
    "types": "dist/index",
    "scripts": {
        "build": "yarn clean && yarn copy && yarn compile",
        "build:watch": "yarn clean && yarn copy && yarn compile -w",
        "clean": "del dist",
        "compile": "../../node_modules/typescript/bin/tsc",
        "copy": "cd src/ && cpy './**/*.sql' --parents ../dist/ && cd ../",
        "prepublishOnly": "yarn build",
        "pretest": "bash ../../scripts/pre-test.sh"
    },
    "dependencies": {
        "@arkecosystem/core-container": "^5.3.0-dev.0",
        "@arkecosystem/core-database": "^5.3.0-dev.0",
        "@arkecosystem/core-event-emitter": "^2.6.31",
        "@arkecosystem/core-interfaces": "^5.3.0-dev.0",
        "@arkecosystem/core-state": "^5.3.0-dev.0",
        "@arkecosystem/core-transactions": "^5.3.0-dev.0",
        "@arkecosystem/core-utils": "^5.3.0-dev.0",
        "@arkecosystem/crypto": "^5.3.0-dev.0",
        "@arkecosystem/utils": "^0.3.0",
        "bluebird": "^3.5.5",
        "cpy-cli": "^2.0.0",
        "dayjs": "^1.8.15",
        "lodash.chunk": "^4.2.0",
        "lodash.partition": "^4.6.0",
        "pg-promise": "^8.7.4",
        "pluralize": "^8.0.0",
        "sql": "^0.78.0"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.27",
        "@types/lodash.chunk": "^4.2.6",
        "@types/lodash.partition": "^4.6.6",
        "@types/pluralize": "^0.0.29"
    },
    "engines": {
        "node": ">=10.x"
    },
    "publishConfig": {
        "access": "public"
    }
}
