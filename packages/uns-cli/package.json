{
    "name": "@uns/uns-cli",
    "description": "uns CLI",
    "version": "0.0.1",
    "author": "Space Elephant",
    "contributors": [
        "Fabien Tr√©guer <fabien@spacelephant.org>"
    ],
    "bin": {
        "uns": "./bin/run"
    },
    "dependencies": {
        "@oclif/command": "^1.5.11",
        "@oclif/config": "^1.12.9",
        "@oclif/plugin-help": "^2.1.6",
        "tslib": "^1.10.0",
        "module-alias": "^2.2.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.21.3",
        "@types/node": "^11.10.5",
        "globby": "^10.0.1",
        "ts-node": "^8.3.0",
        "typescript": "^3.3.3333"
    },
    "engines": {
        "node": ">=10.x"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "oclif": {
        "commands": "./dist/commands",
        "bin": "uns",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "scripts": {
        "uns": "./bin/run",
        "prepublishOnly": "yarn build",
        "pretest": "bash ../../scripts/pre-test.sh",
        "prepack": "../../node_modules/.bin/oclif-dev manifest && npm shrinkwrap",
        "postpack": "rm -f oclif.manifest.json",
        "compile": "../../node_modules/typescript/bin/tsc",
        "build": "yarn clean && yarn compile",
        "build:watch": "yarn clean && yarn compile -w",
        "clean": "del dist"
    },
    "types": "dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    }
}
